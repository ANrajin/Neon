<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="01a40709-8802-40f9-957f-741fce82a3a0" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2021.1/project-config.xsd">
  <name>Development</name>
  <description />
  <settings>
    <options>
      <option name="cleanBuild" value="true" />
    </options>
    <parameters />
    <build-runners>
      <runner id="RUNNER_2" name="Restore" type="dotnet">
        <parameters>
          <param name="command" value="restore" />
          <param name="dotNetCoverage.dotCover.home.path" value="%teamcity.tool.JetBrains.dotCover.CommandLineTools.DEFAULT%" />
          <param name="paths" value="src/Neon.sln" />
          <param name="required.sdk" value="6" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
      <runner id="RUNNER_1" name="build" type="dotnet">
        <parameters>
          <param name="command" value="build" />
          <param name="configuration" value="Debug" />
          <param name="dotNetCoverage.dotCover.home.path" value="%teamcity.tool.JetBrains.dotCover.CommandLineTools.DEFAULT%" />
          <param name="framework" value="net6.0" />
          <param name="paths" value="src/Neon.sln" />
          <param name="required.sdk" value="6" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="Neon_Development" />
    </vcs-settings>
    <requirements />
    <build-triggers>
      <build-trigger id="TRIGGER_1" type="vcsTrigger">
        <parameters>
          <param name="enableQueueOptimization" value="true" />
          <param name="quietPeriodMode" value="USE_DEFAULT" />
          <param name="triggerRules" value="+:root=Neon_Development:**" />
        </parameters>
      </build-trigger>
    </build-triggers>
    <build-extensions>
      <extension id="swabra" type="swabra">
        <parameters>
          <param name="swabra.enabled" value="swabra.before.build" />
          <param name="swabra.processes" value="kill" />
        </parameters>
      </extension>
      <extension id="BUILD_EXT_1" type="VcsLabeling">
        <parameters>
          <param name="labelingPattern" value="build-%system.build.number%" />
          <param name="successfulOnly" value="true" />
          <param name="vcsRootId" value="Neon_Development" />
        </parameters>
      </extension>
      <extension id="BUILD_EXT_2" type="notifications">
        <parameters>
          <param name="buildFinishedFailure" value="true" />
          <param name="email" value="rajin.exe@gmail.com" />
          <param name="notifier" value="email" />
        </parameters>
      </extension>
    </build-extensions>
    <cleanup />
  </settings>
</build-type>

